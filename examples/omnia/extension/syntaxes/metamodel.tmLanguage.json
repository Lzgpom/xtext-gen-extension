  "repository": {
    "comment": {
      "patterns": [
        {
          "match": "//.*$",
          "name": "comment.line"
        },
        {
          "name": "comment.block",
          "begin": "/\\*",
          "end": "\\*/"
        }
      ]
    },
    "string": {
      "patterns": [
        {
          "name": "string.quoted.single",
          "begin": "\u0027",
          "end": "\u0027"
        },
        {
          "name": "string.quoted.double",
          "begin": "\"",
          "end": "\""
        }
      ]
    },
    "number": {
      "patterns": [
        {
          "match": "\\b[0-9]+\\b\\.{0,1}",
          "name": "constant.numeric"
        }
      ]
    },
