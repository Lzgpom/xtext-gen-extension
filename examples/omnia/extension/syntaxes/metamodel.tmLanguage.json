    "bm",
    "bamol"
  ],
  "scopeName": "source.metamodel",
  "patterns": [
    {
      "include": "#comment"
    },
    {
      "include": "#string"
    },
    {
      "include": "#number"
    },
    {
      "include": "#language_keyword"
    },
    {
