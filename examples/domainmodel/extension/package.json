{
  "name": "domainmodel",
  "displayName": "Domainmodel",
  "description": "Domainmodel Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:domainmodel"
  ],
  "main": "src/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "domainmodel",
        "aliases": [
          "domainmodel"
        ],
        "extensions": [
          "dmodel"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "domainmodel",
        "scopeName": "source.domainmodel",
        "path": "./syntaxes/domainmodel.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "vscode-languageclient": "^7.0.0"
  }
}